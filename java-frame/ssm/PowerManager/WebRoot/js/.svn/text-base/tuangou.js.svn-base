function show(element) {
    if(element.parentNode.getElementsByTagName("DIV")[1]!=null)
      element.parentNode.getElementsByTagName("DIV")[1].className = "show";
  
      
}

function hidden(element) {
  if(element.parentNode.getElementsByTagName("DIV")[1]!=null)
    element.parentNode.getElementsByTagName("DIV")[1].className = "hidden";
