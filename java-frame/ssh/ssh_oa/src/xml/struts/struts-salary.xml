<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="salary" extends="struts-default" >
		<action name="salary*" class="com.baidu.salary.action.SalaryAction" method="{1}">
			<result name="list">/WEB-INF/salary/list.jsp</result>
			<result name="add">/WEB-INF/salary/add.jsp</result>
			<result name="modify">/WEB-INF/salary/modify.jsp</result>
			<result name="upload">/WEB-INF/salary/upload.jsp</result>
		</action>
	</package>
</struts>