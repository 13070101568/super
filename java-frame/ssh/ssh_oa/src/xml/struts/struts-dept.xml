<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="dept" extends="struts-default" >
		<action name="dept*" class="com.baidu.dept.action.DeptAction" method="{1}">
			<result name="depttree">/WEB-INF/dept/depttree.jsp</result>
			<result name="list">/WEB-INF/dept/list.jsp</result>
			<result name="edit">/WEB-INF/dept/edit.jsp</result>
			<result name="modify">/WEB-INF/dept/modify.jsp</result>
			<result name="index">/index.jsp</result>
			<result name="success">/success.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
	</package>
</struts>